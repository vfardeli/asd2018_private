<?xml version = "1.0" encoding = "utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name = "org.alignprivate.asd.model.CurrentMajors" table="CurrentMajors">

        <meta attribute = "class-description">
            This class contains the Work Experience detail.
        </meta>
        <id name = "currentMajorId" column="CurrentMajorId" type="int">
            <generator class="native"/>
        </id>

        <many-to-one name="student" class="org.alignprivate.asd.model.Students"
                     column="NeuId" />

        <many-to-one name="major" class="org.alignprivate.asd.model.Majors"
                     column="MajorId" />
    </class>
</hibernate-mapping>